<!DOCTYPE struts PUBLIC
"-//Apache Software Foundation//DTD Struts Configuration 2.0//EN"
"http://struts.apache.org/dtds/struts-2.0.dtd">

<!-- Author: Antonio -->
 
<struts>
    <!-- Add your configuration elements here -->
    
    <package name="valoracion" namespace="/valoracion" extends="struts-default">
        <!--<action name="HelloWorld" class="example.HelloWorld">
            <result>/example/HelloWorld.jsp</result>
        </action>-->
        <action name="crearValoracion" class="controllers.RegistrarValoracionAction" method="mostrarEventos">
            <result name="success">/vista/crearValoracion.jsp</result>
            <result name="error">/vista/opcionesVoluntario.jsp</result>
        </action>
        <action name="registrarValoracion" class="controllers.RegistrarValoracionAction" method="registrarValoracion">
            <result name="success">/vista/registrarValoracion.jsp</result>
            <result name="error">/vista/opcionesVoluntario.jsp</result>
        </action>
        <action name="guardarValoracion" class="controllers.RegistrarValoracionAction" method="guardarValoracion">
            <result name="success">/vista/OpcionesVoluntario.jsp</result>
            <result name="error">/vista/opcionesVoluntario.jsp</result>
        </action>
    </package>
</struts>
